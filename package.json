    "@inertiajs/progress": "^0.1.2",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@vue/compiler-sfc": "^3.0.2",
    "animejs": "^3.2.1",
    "babel-plugin-prismjs": "^1.0.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^5.2.2",
    "laravel-mix": "^v6.0.0-beta.10",
    "postcss": "^8.2.10",
    "prismjs": "^1.22.0",
    "puppeteer": "^2.1.1",
    "sass": "^1.32.8",
